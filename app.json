{
  "expo": {
    "name": "MissGastos",
    "slug": "MissGastos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/presentation/assets/icons/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/presentation/assets/icons/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/presentation/assets/icons/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/presentation/assets/icons/icon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./src/presentation/app"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
