{
  "expo": {
    "name": "MissGastos",
    "slug": "MissGastos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./presentation/assets/icons/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./presentation/assets/icons/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./presentation/assets/icons/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon":  "./presentation/assets/icons/icon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./presentation/app"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
